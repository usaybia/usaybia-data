         <persName ref="https://usaybia.net/person/1726"></persName>
         <birth>
            <choice>
               <reg>
                  <date when="" calendar="#julian">1160 or 1161</date>
               </reg>
               <orig>
                  <date when="" calendar="#hijri">555 or 556</date>
               </orig>
            </choice>
            <note type="desc">
               <persName ref="https://usaybia.net/person/1726">al-Qāḍī Nafīs al-Dīn ibn al-Zubayr</persName>
               was born either in the year 1160 or 1161.
            </note>
         </birth>
      </person>
   </listPerson>
   <bibl type="urn">
