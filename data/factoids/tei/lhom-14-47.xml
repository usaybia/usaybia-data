<ab type="factoid" subtype="residence" xml:id="factoid-5" resp="#mtolay"><!--not residence-->
   <idno type="URI">https://usaybia.net/factoid/14-47-5</idno>
   <listPerson>
      <person>
         <persName ref="https://usaybia.net/person/1726"></persName>
         <occupation ana="http://syriaca.org/keyword/occupations">
            <note type="desc">
               <persName ref="https://usaybia.net/person/1726">al-Qāḍī Nafīs al-Dīn ibn al-Zubayr</persName> was appointed from <persName ref="https://usaybia.net/person/1451">al-Malik al-ʿĀdil</persName> 
               as chief physician. He was known for his treatments of eye alignments in the 
               <placename ref="https://usaybia.net/place/416">Nāṣirī Hospital</placename>.
            </note>
         </occupation>
      </person>
   </listPerson>
   <bibl type="urn">
      <ptr target="urn:cts:arabicLit:0668IbnAbiUsaibia.Tabaqatalatibba.lhom-ed-ara1:14.47"></ptr>
      <citedRange unit="part">2</citedRange>
   </bibl>
</ab>

