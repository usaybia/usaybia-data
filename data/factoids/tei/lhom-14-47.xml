         <death>
            <placeName ref="https://usaybia.net/place/121"></placeName>
            <choice>
               <reg>
                  <date notBefore="1222" notAfter="1242" calendar="#julian">1223-1242</date>
               </reg>
               <orig>
                  <date notBefore="620" notAfter="640" calendar="#hijri">630's</date>
               </orig>
            </choice>
            <note type="desc">
               <persName ref="https://usaybia.net/person/1726">al-Qāḍī Nafīs al-Dīn ibn al-Zubayr</persName>
               died between the years 1223 to 1242  in
               <placeName ref="https://usaybia.net/place/121">Cairo</placeName>
               .
            </note>
         </death>
      </person>
   </listPerson>
